/**list_common.wxss**/
page{
    height: 100%;
    width: 100%;
    background: #f4f4f4;
}

.container{
  height: 100%;
  width: 100%;
}

.totle{
  background: #efefef;
  text-align: center;
  width: 100%;
  position: fixed;
  color: blue;
  top: 0;
  left:0;
  padding-bottom: 35rpx;
}
.total.bottom0{
  bottom: 0;
}
.totle .tr{
  background: #ffffff;
}
.table {
  border: 0px solid darkgray;        background:#c7f1e7;
}
.tr {
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
}
.tr_top {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 4rem;
  align-items: center;
}
.td {
  width:40%;
  justify-content: center;
  text-align: center;
  color: #666666;
}
.th {
  width: 40%;
  color: #31c4a1;
  font-size:32rpx;
  font-weight: bold;
}
.address-list{
  background-size: auto 10.5rpx;
  padding-bottom:180rpx;
}

.address-list .item{
  height: 156.55rpx;
  align-items: center;
  display: flex;
  border-bottom: 1rpx solid #DCD9D9;
}

.address-list .l{
  width: 125rpx;
  height: 80rpx;
  overflow: hidden;
}

.address-list .name{
  width: 125rpx;
  height: 43rpx;
  font-size: 29rpx;
  color: #333;
  margin-bottom: 5.2rpx;
      text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}

.address-list .default{
  width: 62.5rpx;
  height: 33rpx;
  line-height: 28rpx;
  text-align: center;
  font-size: 20rpx;
  color: #b4282d;
  border: 1rpx solid #b4282d;
  visibility: visible;
}

.address-list .c{
  flex: 1;
  height: auto;
  overflow: hidden;
}

.address-list .mobile{

  height: 29rpx;
  font-size: 29rpx;
  line-height: 29rpx;
  overflow: hidden;
  color: #333;
  margin-bottom: 6.25rpx;
}

.address-list .address{
  height: 37rpx;
  font-size: 25rpx;
  line-height: 37rpx;
  overflow: hidden;
  color: #666;
}

.address-list .r{
  width: 52rpx;
  height: auto;
  overflow: hidden;
  margin-right: 16.5rpx;
}

.total{
  position: fixed;
  bottom: 0rpx;
  color: #f33d3d;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 30rpx;
  width: 100%;
  background: #fff;
  /* border-top: 1rpx solid #DCD9D9; */
}
.address-list .del{
  /* display: block; */
  width: 52rpx;
  height: 52rpx;
}
.add-vaccine, .add-address{
  background:#31c4a1;
  text-align: center;
  width: 100%;
  height: 99rpx;
  line-height: 99rpx;
  position: fixed;
  border-radius: 0;
  border: none;
  color: #fff;
  font-size: 29rpx;
  bottom: 0;
  left:0;
}

.empty-view{
  height: 100%;
  width: 100%;
  display: flex;
  /* flex-direction: column;*/
  align-items: center;
  justify-content: center;
  background:#fff;
}

.empty-view .icon{
  height: 50rpx;
  width: 58rpx;
}

.empty-view .text{
  width: auto;
  font-size: 30rpx;
  line-height: 0;
  margin-left: 15rpx;
  color: #999;
}