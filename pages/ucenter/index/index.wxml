<view class="container">
  <view class="profile-info" bindtap="goLogin">
    <image class='ucenter-bg' src='../../../static/images/index/ucenter-bg.png'></image>
    <view class="info">
      <image class="avatar" src="{{userInfo.avatarUrl}}"></image>
      <text class="name">{{userInfo.nickName}}</text> 
    </view> 
  </view>

  <view class="user-menu">
    <!-- <view class="weui-cells__title">权限账户</view> -->
    <navigator class="navigator user">{{powerInfo}}</navigator>
  </view>
  <view class="logout" bindtap="exitLogin">退出当前权限账户</view>
</view>